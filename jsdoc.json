{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["client", "server"],
    "exclude": ["node_modules", "dist", "client/assets"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "opts": {
  "template": "templates/default",
  "encoding": "utf8",
  "destination": "./doc/",
  "recurse": true
  }
}